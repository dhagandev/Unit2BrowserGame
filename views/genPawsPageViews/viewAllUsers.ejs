<div class = "all-users-table">
	
	<% if (users.length !== 0) { %>
		<table>
			<thead>
				<tr>
					<th>User</th>
					<th>Currently Owns</th>
					<th>Money</th>
				</tr>
			</thead>
			<tbody>
				<% users.forEach((user) => { %>
					<tr onclick="window.location='/users/<%= user._id %>';">
						<td><%= user.name %></td>
						<td><%= user.dogs.length %> <%= user.dogs.length === 1 ? "Dog" : "Dogs" %></td>
						<td>$<%= user.money %></td>
					</tr>
				<% }) %>
			</tbody>
		</table>
	<% } else { %>
		<div class="no-table">Oh no! We have no members!</div>
	<% } %>
</div>